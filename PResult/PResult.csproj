<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net6.0;net7.0</TargetFrameworks>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Title>Implementation of Result pattern</Title>
        <Authors>PriTexX</Authors>
        <PackageLicenseUrl>https://github.com/PriTexX/presult/blob/main/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/PriTexX/presult</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>result;error handling;presult;Result;exception</PackageTags>
        <Description>Provides a nice way of handling application errors with `Result` pattern</Description>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DocumentationFile>bin\Release\PResult.xml</DocumentationFile>
    </PropertyGroup>


</Project>
